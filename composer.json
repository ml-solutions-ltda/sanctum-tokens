{
  "name": "mlsolutions/sanctum-tokens",
  "description": "A Laravel Nova resource tool for sanctum tokens.",
  "keywords": [
    "laravel",
    "nova",
    "sanctum"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1|^8.2|^8.3|^8.4",
    "laravel/nova": "^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "MlSolutions\\SanctumTokens\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MlSolutions\\SanctumTokens\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "laravel/pint": "^1.14"
  }
}
